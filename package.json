{
  "name": "itunes-discord-integration",
  "version": "0.4.0",
  "description": "A macOS application to set the activity in your Discord profile to the currently played song on iTunes",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder -m --dir",
    "dist": "electron-builder -m"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AngryKiller/iTunes-Discord-integration.git"
  },
  "keywords": [
    "itunes",
    "macos",
    "discord",
    "music",
    "electron"
  ],
  "author": "AngryKiller",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/AngryKiller/iTunes-Discord-integration/issues"
  },
  "homepage": "https://github.com/AngryKiller/iTunes-Discord-integration#readme",
  "dependencies": {
    "discord-rich-presence": "0.0.6",
    "electron-photon": "^0.1.0",
    "electron-settings": "^3.1.4",
    "electron-updater": "^2.21.1",
    "itunes-bridge": "^0.2.2",
    "kofiloop": "^1.0.1",
    "path": "^0.12.7",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "electron": "1.8.3",
    "electron-builder": "^20.5.0"
  },
  "build": {
    "appId": "fr.angrykiller.itunes-discord",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "productName": "iTunes⇄Discord",
    "copyright": "Copyright © 2018 ${author}"
  }
